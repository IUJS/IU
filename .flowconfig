[ignore]
.*/node_modules/.*
.*/test/.*
.*/scripts/.*
.*/examples/.*
.*/benchmarks/.*

[include]

[libs]
flow

[lints]
# all=off by default
all=warn
untyped-type-import=error
sketchy-null=warn
sketchy-null-bool=off

[options]
unsafe.enable_getters_and_setters=true
# module.name_mapper='^compiler/\(.*\)$' -> '<PROJECT_ROOT>/src/compiler/\1'
suppress_comment= \\(.\\|\n\\)*\\$flow-disable-line
